@model WebSoccer.Models.Players

@{
    ViewBag.Title = "Excluir jogador";
}

@Html.Partial("_ViewSession")


<h2>Deseja realmente excluir esse jogador?</h2>
<div>
    
    <hr />
   
    @using (Html.BeginForm()) {
